<script>
	function handleLogout() {
		   if (confirm('Are you sure you want to logout?')) {
			   showNotification('Logging out...');
			   
			   setTimeout(() => {
				   window.location.href = 'login.html'; 
			   }, 1000);
		   }
	   }
</script>
